      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_PARENTHS)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comma",
      "scope": [
        "punctuation.separator.comma"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_COMMA)",
        "fontStyle": "italic"
      }
