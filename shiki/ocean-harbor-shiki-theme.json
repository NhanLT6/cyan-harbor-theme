      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_PARAMETER)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Instance Field",
      "scope": [
        "variable.other.property",
        "variable.other.member"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_INSTANCE_FIELD)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Static Field",
      "scope": [
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_STATIC_FIELD)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "punctuation.section.braces",
        "punctuation.definition.block"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_BRACES)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.section.brackets",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_BRACKETS)",
