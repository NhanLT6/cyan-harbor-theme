{
  "name": "Ocean Harbor",
  "type": "dark",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#b8c5d0",
    "editor.lineHighlightBackground": "#1B2529",
    "editor.selectionBackground": "#314549",
    "editorCursor.foreground": "#FFCC00",
    "editorLineNumber.foreground": "#475F63",
    "editorLineNumber.activeForeground": "#607D86",
    "editorIndentGuide.background": "#37474F",
    "editorIndentGuide.activeBackground": "#009688",
    "editorWhitespace.foreground": "#65737E"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_LINE_COMMENT)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Block Comment",
      "scope": [
        "comment.block"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_BLOCK_COMMENT)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Doc Comment",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_DOC_COMMENT)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_KEYWORD)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "punctuation.operator"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_OPERATION_SIGN)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_STRING)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String Escape",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_VALID_STRING_ESCAPE)",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_NUMBER)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant.language",
        "constant.other"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_CONSTANT)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Call",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_FUNCTION_CALL)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Function Declaration",
      "scope": [
        "entity.name.function",
        "meta.function"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_FUNCTION_DECLARATION)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Method",
      "scope": [
        "entity.name.function.member"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_INSTANCE_METHOD)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Static Method",
      "scope": [
        "entity.name.function.static"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_STATIC_METHOD)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class Name",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_CLASS_NAME)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class Reference",
      "scope": [
        "entity.name.type",
        "support.type"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_CLASS_REFERENCE)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Interface Name",
      "scope": [
        "entity.name.type.interface"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_INTERFACE_NAME)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Abstract Class",
      "scope": [
        "entity.name.type.class.abstract"
      ],
      "settings": {
        "foreground": "var(--token-ABSTRACT_CLASS_NAME_ATTRIBUTES)",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable",
        "variable.other"
      ],
      "settings": {
        "foreground": "var(--token-DEFAULT_IDENTIFIER)",
